{
  "rewrites": [
    {
      "source": "/RevuAI/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/RevuAI/assets/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}